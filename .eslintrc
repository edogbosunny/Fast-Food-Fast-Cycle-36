{
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
  
    "extends": "airbnb",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
  
    "rules": {
      "no-console": "warn",
      "max-len": [2, 300, 4],
      "prefer-destructuring":"off",
      "no-unused-vars":"off",
      "no-param-reassign": 0
    }
  }
  